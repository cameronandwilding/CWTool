<?php

/**
 * @file
 * Allows users to create checklist.
 */

/**
* Implements hook_menu().
*/
function checklist_menu() {
  $items['admin/config/checklist'] = array(
    'title' => 'Checklist',
    'description' => 'The checklist admin page section',
    'route_name' => 'checklist.admin',
  );
  return $items;
}
?>
